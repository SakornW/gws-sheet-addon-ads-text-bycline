{
  "oauthScopes": [
    "https://www.googleapis.com/auth/script.container.ui",
    "https://www.googleapis.com/auth/spreadsheets.currentonly",
    "https://www.googleapis.com/auth/script.external_request",
    "https://www.googleapis.com/auth/userinfo.email",
    "https://www.googleapis.com/auth/script.locale",
    "https://www.googleapis.com/auth/spreadsheets.readonly"
  ],
  "addOns": {
    "common": {
      "name": "Ads Text Generator (HTTP)",
      "logoUrl": "https://www.gstatic.com/images/icons/material/system/1x/extension_white_24dp.png",
      "layoutProperties": {
        "primaryColor": "#2772E4",
        "secondaryColor": "#F2F6FF"
      },
      "useLocaleFromApp": true,
      "homepageTrigger": {
        "runFunction": "https://<YOUR_BACKEND_URL>/gws/homepage",
        "enabled": true
      },
      "universalActions": []
    },
    "sheets": {
      "onFileScopeGrantedTrigger": {
        "runFunction": "https://<YOUR_BACKEND_URL>/gws/onFileScopeGranted"
      },
      "contextualTriggers": []
    }
  },
  "schemaVersion": "v1"
}
